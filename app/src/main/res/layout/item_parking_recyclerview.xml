<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_marginBottom="20dp"
    android:background="@color/white"
    android:padding="10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/parkingName_textView"
        android:text="Parking Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/parkingSlots_textView"
        android:text="Total Parking Slots : "
        android:layout_below="@+id/parkingName_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/parkingSlotNumber_textView"
        android:layout_toRightOf="@+id/parkingSlots_textView"
        android:layout_below="@+id/parkingName_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/parkingSubAdmin_textView"
        android:layout_below="@+id/parkingSlotNumber_textView"
        android:text="Sub Admin : "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/parkingSubAdminName_textView"
        android:layout_toRightOf="@+id/parkingSubAdmin_textView"
        android:layout_below="@+id/parkingSlotNumber_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/edit_textView"
        android:padding="10dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/parkingSubAdmin_textView"
        android:text="Edit Admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</RelativeLayout>